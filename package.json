{
  "name": "helen-frontend",
  "version": "1.0.0",
  "description": "a boilerplate for TS and Snowpack bundler",
  "main": "index.tsx",
  "scripts": {
    "start": "snowpack dev",
    "build": "snowpack build",
    "clean": "rimraf ./build",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "test": "web-test-runner \"src/**/*.test.{js,jsx,ts,tsx}\" --node-resolve --puppeteer"
  },
  "keywords": [
    "parcel"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "CMIV",
  "license": "GPLv3",
  "dependencies": {
    "@auth0/auth0-react": "^1.8.0",
    "axios": "^0.24.0",
    "history": "^5.0.1",
    "preact": "^10.5.13"
  },
  "devDependencies": {
    "@esm-bundle/chai": "^4.3.4-fix.0",
    "@prefresh/snowpack": "^3.0.0",
    "@snowpack/plugin-dotenv": "^2.1.0",
    "@snowpack/plugin-sass": "1.4.0",
    "@snowpack/plugin-typescript": "^1.2.1",
    "@snowpack/web-test-runner-plugin": "^0.2.2",
    "@testing-library/preact": "^2.0.1",
    "@types/chai": "^4.2.17",
    "@types/mocha": "^8.2.2",
    "@types/react": "^17.0.26",
    "@types/snowpack-env": "2.3.3",
    "@web/test-runner": "^0.13.3",
    "@web/test-runner-puppeteer": "^0.10.2",
    "bulma": "^0.9.3",
    "chart.js": "^3.4.1",
    "prettier": "^2.2.1",
    "react-chartjs-2": "^3.0.3",
    "rimraf": "^3.0.2",
    "snowpack": "^3.8.8",
    "typescript": "^4.3.4",
    "wouter-preact": "^2.7.4"
  },
  "alias": {
    "react": "preact-compat",
    "react-dom": "preact-compat"
  }
}
